{
  "name": "noteworks",
  "version": "1.0.0",
  "description": "NoteWorks - A notes management application",
  "main": "index.js",
  "scripts": {
    "start": "node backend/index.js",
    "build": "cd frontend && npm install && npm run build",
    "heroku-postbuild": "npm run build",
    "serve": "node server.js",
    "frontend": "cd frontend/noteworks && npm run dev",
    "server": "cd backend && npm start",
    "final": "concurrently \"npm run frontend\" \"npm run server\""
  },
  "dependencies": {
    "axios": "^1.7.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.3",
    "react-icons": "^5.2.1",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.24.0",
    "serve-static": "^1.14.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.3"
  },
  "author": "",
  "license": "ISC"
}
